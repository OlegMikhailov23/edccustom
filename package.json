{
  "name": "EDC Customs",
  "version": "1.0.0",
  "private": true,
  "description": "EDC Customs on-line shop",
  "devDependencies": {
    "@htmlacademy/editorconfig-cli": "1.x",
    "autoprefixer": "^7.2.6",
    "browser-sync": "^2.23.7",
    "gulp": "3.9.x",
    "gulp-autoprefixer": "^6.0.0",
    "gulp-cache": "^1.0.2",
    "gulp-less": "3.4.x",
    "gulp-plumber": "1.2.x",
    "gulp-postcss": "^7.0.1",
    "stylelint": "8.4.x",
    "stylelint-config-htmlacademy": "0.1.x"
  },
  "scripts": {
    "editorconfig": "editorconfig-cli",
    "stylelint": "stylelint 'source/less/**/*.less' --syntax less",
    "test": "npm run editorconfig && npm run stylelint",
    "build": "gulp build",
    "start": "npm run build && gulp serve"
  },
  "browserslist": [
    "last 2 versions",
    "IE 11",
    "Firefox ESR"
  ],
  "editorconfig-cli": [
    "*.json",
    "*.js",
    "source/*.html",
    "source/js/**/*.js",
    "source/img/**/*.svg",
    "source/less/**/*.less"
  ],
  "engines": {
    "node": "8.9"
  },
  "dependencies": {
    "ajv": "^6.5.3",
    "ajv-keywords": "^3.2.0",
    "css-mqpacker": "^7.0.0",
    "del": "^3.0.0",
    "gulp-csso": "^3.0.1",
    "gulp-imagemin": "^4.1.0",
    "gulp-rename": "^1.4.0",
    "run-sequence": "^2.2.1"
  }
}
